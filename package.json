    "mkdirp": "^0.5.5",
    "ps-node": "^0.1.6",
    "vdf-parser": "^1.1.0",
    "winreg": "^1.2.4",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.1",
    "@types/jquery": "^3.5.5",
    "@types/klaw-sync": "^6.0.0",
    "@types/linkifyjs": "^2.1.3",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^15.12.1",
