    "linkify",
    "linkifyjs",
    "loadlib",
    "lobsterosity",
    "localconfig",
    "localstorage",
    "mgln",
    "Moto",
    "negi",
    "Niklas",
    "noot",
    "paramiko",
    "psutil",
    "rankedsoloreset",
    "rungameid",
    "skel",
    "spammy",
    "steamworks",
    "thereisnofuture",
    "thoday",
    "tooltipster",
    "tooltipstered",
    "unban",
    "unmarshal",
    "unmaximize",
    "unshutdown",
    "Voss",
    "WASAPI"
  ]
